{{# each products }}
<div class="row mt-5">
  {{# each this }}
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img class="img-responsive"  src="{{ this.imagePath }}" alt="breakfast at acropolis" width="300px">
      <div class="caption">
        <h3>{{ this.title }}</h3>
        <p class="description">{{ this.description }}</p>
        <div class="container d-flex clearfix">
          <div class="price">${{ this.price }}</div>
          <a href="#" class="btn btn-success ml-auto"  role="button">Add to Shopping Cart</a>
        </div>

      </div>
    </div>
  </div>
  {{/each}}
  
</div>
{{/each}}


